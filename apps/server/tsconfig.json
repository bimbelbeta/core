{
	"extends": "@bimbelbeta/config/tsconfig.base.json",
	"compilerOptions": {
		"lib": ["ESNext", "DOM"],
		"composite": true,
		"tsBuildInfoFile": "dist/.tsbuildinfo",
		"outDir": "dist",
		"strictNullChecks": true,
		"paths": {
			"@/*": ["./src/*"],
			"*": ["./*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	}
}
